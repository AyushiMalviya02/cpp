#include <iostream>
using namespace std;

// Function to find GCD using Euclidean algorithm
long long gcd(long long a, long long b) {
    while (b != 0) {
        long long temp = b;
        b = a % b;
        a = temp;
    }
    return a;
}

int main() {
    long long a, b;
    cout << "Enter two numbers: ";
    cin >> a >> b;

    long long lcm = (a * b) / gcd(a, b);
    cout << "LCM is: " << lcm;

    return 0;
}
