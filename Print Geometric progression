#include <iostream>
#include <cmath>
using namespace std;

int main() {
    double a, r;
    int n;

    cout << "Enter first term, common ratio, and number of terms: ";
    cin >> a >> r >> n;

    cout << "Geometric Progression: ";
    for (int i = 0; i < n; i++) {
        cout << a * pow(r, i) << " ";
    }

    return 0;
}
