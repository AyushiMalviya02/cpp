#include <iostream>
using namespace std;

int main() {
    int n;
    cout << "Enter the number of rows: ";
    cin >> n;

    for (int i = 1; i <= n; i++) {        // rows
        for (int j = 1; j <= i; j++) {    // numbers in row
            cout << j << " ";
        }
        cout << endl;
    }

    return 0;
}
