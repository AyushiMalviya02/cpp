#include <iostream>
using namespace std;

int main() {
    int a, d, n;
    cout << "Enter first term, common difference, and number of terms: ";
    cin >> a >> d >> n;

    cout << "Arithmetic Progression: ";
    for (int i = 0; i < n; i++) {
        cout << a + i * d << " ";
    }

    return 0;
}
