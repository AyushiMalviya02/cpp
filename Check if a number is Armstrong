#include <iostream>
#include <cmath>
using namespace std;

int main() {
    long long n;
    cout << "Enter a number: ";
    cin >> n;

    long long original = n;
    int digits = 0;
    long long sum = 0;

    // Count digits
    long long temp = n;
    while (temp != 0) {
        digits++;
        temp /= 10;
    }

    // Calculate sum of digits^digits
    temp = n;
    while (temp != 0) {
        int digit = temp % 10;
        sum += pow(digit, digits);
        temp /= 10;
    }

    if (sum == original)
        cout << original << " is an Armstrong number";
    else
        cout << original << " is not an Armstrong number";

    return 0;
}
