#include <iostream>
using namespace std;

int main() {
    int n;
    cout << "Enter the number of rows: ";
    cin >> n;

    int num = 1; // Start from 1

    for (int i = 1; i <= n; i++) {        // rows
        for (int j = 1; j <= i; j++) {    // columns in each row
            cout << num << " ";
            num++;
        }
        cout << endl;
    }

    return 0;
}
